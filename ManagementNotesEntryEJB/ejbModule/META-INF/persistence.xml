<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
	<persistence-unit name="gsn">
	 <!-- provider>org.hibernate.ejb.HibernatePersistence</provider-->
	 <!-- provider>org.eclipse.peristence.jpa.PersistenceProvider</provider-->
		<jta-data-source>jdbc/gsn</jta-data-source>
		<class>tn.rnu.isetsf.model.Utilisateur</class>
		<class>tn.rnu.isetsf.model.Enseignant</class>
		<class>tn.rnu.isetsf.model.Etudiant</class>
		<class>tn.rnu.isetsf.model.Formation</class>
		<class>tn.rnu.isetsf.model.Administrateur</class>
		<class>tn.rnu.isetsf.model.Role</class>
